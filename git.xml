<?xml version="1.0" encoding="UTF-8"?>
<framework xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:noNamespaceSchemaLocation="schemas/frameworkDescriptionVersion1.1.xsd" name="git"
           invoke="C:\Program Files (x86)\Git\bin\git.exe" alias="git" enabled="true" version="1">
    <command>
        <name>init</name>
    </command> 
    <command>
        <name>clone</name>
    </command>    
    <command>
        <name>add</name>
    </command>
    <command>
        <name>blame</name>
        <params>file</params>        
    </command>    
    <command>
        <name>status</name>
    </command>
    <command>
        <name>revert HEAD</name>
    </command>  
    <command>
        <name>revert</name>
        <params>id</params> 
    </command>   
    <command>
        <name>checkout</name>
        <params>id</params> 
    </command>   
    <command>
        <name>merge</name>
        <params>branch_name</params> 
    </command>    
    <command>
        <params></params>
        <name>log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short</name>
    </command>
    <command>
        <name>branch</name>
        <params>branch_name[=null]</params>
    </command>
    <command>
        <name>branch -d</name>
        <params>branch_name</params>
    </command>    
    <command>
        <name>commit -m</name>
        <params>"fixed"</params>
    </command>
    <command>
        <name>push origin</name>
        <params>branch_name[=null]</params>
    </command>
</framework>
